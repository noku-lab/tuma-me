{
  "name": "tuma-me",
  "version": "1.0.0",
  "description": "Centralized platform-escrow model React Native app",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "install:all": "npm install",
    "start": "cd frontend && npm run start:tunnel",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start:frontend:tunnel": "cd frontend && npm run start:tunnel",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "dev:tunnel": "concurrently \"npm run start:backend\" \"npm run start:frontend:tunnel\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "overrides": {
    "react-native": "0.81.5",
    "@react-native/virtualized-lists": "0.81.5",
    "expo": "54.0.31",
    "expo-font": "14.0.11",
    "expo-asset": "12.0.12",
    "expo-constants": "18.0.13",
    "expo-file-system": "19.0.21",
    "expo-keep-awake": "15.0.8",
    "expo-modules-core": "3.0.29",
    "@expo/vector-icons": "15.0.3",
    "@expo/fingerprint": "0.15.4",
    "react": "19.1.0"
  }
}
